// print following pattern
// A
// B B
// C C C
// D D D D
// E E E E E

#include <stdio.h>

int main() {
    // Write C code here
    char ch='A';                //A == 65
    for (int i=1; i<=5; i++)
    {
        for (int j=1;j<=i;j++)
        {
            printf("%c",ch);    //prints the same character everytime
        }
        printf("\n");           //next line
        ch++;                   //need to increment character after each line
    }
    
    return 0;
}
